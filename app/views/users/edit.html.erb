<h1>Edit User</h1>

<div class="row">
  <div class="col-md-6">
    <%= form_for(@user) do |f| %>
      
      <%= render 'shared/errors', object: f.object %>

      <%= f.label :username %>
      <%= f.text_field :username, class: "form-control" %>

      <%= f.label :email %>
      <%= f.text_field :email, class: "form-control" %>

      <%= f.submit "Submit", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

